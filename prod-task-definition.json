{
    "family": "WalletServiceApiTaskFamily",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::account_id:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "WalletServiceApiProd",
        "image": "948104088074.dkr.ecr.us-east-1.amazonaws.com/walletserviceapi:tag",
        "cpu": 256,
        "memory": 512,
        "portMappings": [
          {
            "containerPort": 80
          },
          {
            "containerPort": 443
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "ASPNETCORE_ENVIRONMENT",
            "value": "Production"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/WalletServiceApi",
            "awslogs-region": "region",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": [ "FARGATE" ],
    "cpu": "1024",
    "memory": "2048",
    "taskRoleArn": "arn:aws:iam::account_id:role/ecsTaskRole",
    "networkConfiguration": {
      "awsvpcConfiguration": {
        "subnets": [
          "subnet-054067f0cf2ac1202",
          "subnet-03401969897eff6a3",
          "subnet-06e40a669cb76ce9f"
        ],
        "securityGroups": [
          "sg-0bf91e315cc06f128"
        ],
        "assignPublicIp": "ENABLED"
      }
    }
  }